/* cmps/nav-bar/nav-bar.wxss */
.container {
  position: fixed;
  top: 0;
  width: 100%;
  z-index: 99;
}

.nav-bar {
  display: flex;
  justify-content: center;
  align-items: center;
  position: relative;
  font-size: 32rpx;
  font-weight: bold;
}

.icons {
  position: absolute;
  left: 0;
  top: 0;
  height: 100%;
  display: flex;
  align-items: center;
  padding: 0 30rpx;
}


.backView{
  width: 64rpx;
  height: 64rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.homeView{
  width: 64rpx;
  height: 64rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.back{
  width: 32rpx;
  height: 32rpx;
}
.home{
  /* margin-left: 40rpx; */
  width: 36rpx;
  height: 36rpx;
}
