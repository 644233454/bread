page {
  height: 100%;
  width: 100%;
  background-color: #f7f7f6;
}

.container {
  height: 100%;
  width: 100%;
  display: flex;
  flex-direction: column;
  position: fixed;
}

/*搜索框*/

.search-wrapper {
  background-color: white;
  width: 100%;
  /*不放大，不缩小,固定高度100rpx*/
  flex: 0 0 110rpx;
  display: flex;
  align-items: center;
}

.input_search {
  padding-left: 20rpx;
  padding-right: 20rpx;
  margin-left: 65rpx;
  height: 70rpx;
  width: 560rpx;
  background-color: #f5f5f5;
  border-radius: 35rpx;
  text-align: center;
}

.searchcion {
  margin-left: 20rpx;
}

.v_tab {
  width: 100%;
  /*不放大，不缩小,固定高度72rpx*/
  flex: 0 0 72rpx;
  background-color: white;
  display: flex;
  align-items: center;
  flex-direction: row;
  justify-content: center;
}

.v_all {
  flex-grow: 1;
  line-height: 1;
  text-align: center;
  height: 72rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.v_fivestar {
  flex-grow: 1;
  line-height: 1;
  text-align: center;
  height: 72rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.v_fourstar {
  flex-grow: 1;
  line-height: 1;
  text-align: center;
  height: 72rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.v_threestar {
  flex-grow: 1;
  line-height: 1;
  text-align: center;
  height: 72rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

#tv_all {
  color: #666;
  font-size: 22rpx;
}

