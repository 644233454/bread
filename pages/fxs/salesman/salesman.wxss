}

.v_TabWrap {
  height: 40rpx;
  width: 98rpx;
  background-color: white;
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius: 20rpx;
}

.v_tabContent {
  width: 100%;
  height: 100%;
}

.v_tabContentList {
  width: 100%;
  /*分配其他元素后，自己充满屏幕*/
  flex: 1;
}

#v_listHead {
  margin-top: 16rpx;
  margin-left: 31rpx;
}

#tv_listHeadOne, #tv_listHeadTwo {
  color: #999;
  font-size: 26rpx;
}

#tv_listHeadSalesmanNumber {
  margin-left: 11rpx;
  margin-right: 11rpx;
  color: #333;
  font-size: 26rpx;
}

.v_item {
  width: 690rpx;
  margin-top: 20rpx;
