Page{
    background-color: #F7F7F6;
}
.container{
    display: flex;
    width: 100%;
    /* height: 400rpx; */
    flex-direction: column;
    align-items: center;
    margin-top: 20rpx;
    background-color: #F7F7F6;

    /* position: relative;
    box-sizing: border-box; */
}
.noDataView{
    display: flex;
    flex-direction: column;
    align-items: center;
}
.addressNoDataImage{
    width: 230rpx;
    height: 175rpx;
    margin-top: 115rpx;
}
.addressNoDataTitle{
    color: #999999;
    font-size: 28rpx;
    margin-top: 80rpx;
}
.bottomView{
    
    width: 100%;
    height: 100rpx;
    position: fixed;
    bottom: 0px;
    background-color: #F3345C;
    color: #ffffff;
    text-align: center;
    line-height: 100rpx;
}
.itemListView{
    display: flex;
    flex-direction: column;
    width: 100%;
}
.itemView{
    width: 100%;
    height: 128rpx;
    display: flex;
    flex-direction: column;
    background-color: #ffffff;
    /* flex-direction: column; */
    align-items: center;
}
.itemViewTop{
    width: 100%;
    height: 126rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
}
.itemViewTopLeft{
    width: 100%;
    height: 100%;
    display: flex;
    align-items: center;
    /* justify-content: space-between; */
}

.imageView{
    display: flex;
    height: 100%;
    justify-content: center;
    align-items: center;
}
.image{
    width: 36rpx;
    height: 36rpx;
    margin-left: 26rpx;
    margin-right: 24rpx;
}
.itemViewInfo{
    display: flex;
    height: 100%;
    flex-direction: column;
    justify-content: center;
}
.userName{
    color: #333333;
    font-size: 26rpx;
    font-weight: bold;
    margin-bottom: 10rpx;
    display: flex;
    align-items: center;
}
.defaultView{
    width: 70rpx;
    /* height: 20rpx; */
    background: rgb(253, 241, 236);
    color: rgb(204, 95, 121);
    /* border: 2rpx solid #F7F7F6;
    border-radius: 2rpx; */
    font-size: 22rpx;
    text-align: center;
    margin-left: 20rpx;
    
}
.userAddress{
    color: #999999;
    font-size: 22rpx;
    margin-top: 6rpx;
}
.editImageView{
    width: 80rpx;
    height: 80rpx;
    margin-right: 10rpx;
    align-items: center;
    justify-content: center;
}
.editImage{
    width: 26rpx;
    height: 28rpx;
    margin-left: 27rpx;
    margin-top: 26rpx;
}
.itemViewLine{
    width: 100%;
    height: 2rpx;
    background-color: #E8E8E8;
    margin-left: 40rpx;
}
